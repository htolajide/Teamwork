{
  "name": "teamwork",
  "version": "1.0.0",
  "description": "A DevC Capstone Project",
  "main": "app.js",
  "scripts": {
    "build": "babel  src --out-dir dist --copy-files",
    "clean": "rm -rf dist && mkdir dist",
    "heroku-postbuild": "npm run clean && npm run build --prod",
    "start": "node dist/app.js",
    "create-tables": "node  ./src/models/tables tables",
    "start:dev": "SET NODE_ENV=production && nodemon --exec babel-node src/app.js",
    "test": "mocha --require @babel/register src/tests/*.js --exit",
    "coverage": "nyc npm test && nyc report --reporter=text-lcov | coveralls"
  },
  "author": "Taofeek Hammed",
  "license": "ISC",
  "engines": {
    "node": "10.16.0",
    "npm": "6.9.0"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "cloudinary": "^1.16.0",
    "cookie-parser": "^1.4.4",
    "debug": "^4.1.1",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "fs": "0.0.1-security",
    "jsend": "^1.1.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.9.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.0",
    "@babel/node": "^7.5.0",
    "@babel/preset-env": "^7.5.2",
    "@babel/register": "^7.7.0",
    "babel-eslint": "^10.0.2",
    "chai": "^4.2.0",
    "chai-http": "^4.0.0",
    "coveralls": "^3.0.7",
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "make-runnable": "^1.3.6",
    "mocha": "^6.2.2",
    "nodemon": "^1.19.1",
    "nyc": "^14.1.1",
    "pg": "^7.12.1",
    "supertest": "^4.0.2"
  }
}
